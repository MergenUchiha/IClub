generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["prismaSchemaFolder"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Status {
  PENDING
  CANCELLED
  VERIFIED

  @@map("status")
}

enum Lesson {
  LESSON1PLACE1
  LESSON1PLACE2
  LESSON2PLACE1
  LESSON2PLACE2
  LESSON3PLACE1
  LESSON3PLACE2
}