generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["prismaSchemaFolder"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Status {
  PENDING
  CANCELLED
  VERIFIED
  COMPLETED

  @@map("status")
}

enum Lesson {
  LESSON1
  LESSON2
  LESSON3
}