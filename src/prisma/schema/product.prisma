model Product {
    id          String  @id() @default(uuid()) @db.Uuid
    name        String  @db.VarChar(25)
    price       Int     @db.Integer

    categoryId      String @db.Uuid @map("category_id")
    category        Category @relation(fields: [categoryId],references: [id],onDelete: Cascade)
    image      Image?
    @@map("product")
}