        "update": "npx npm-check --update",
        "build": "nest build",
        "seed": "tsc && dotenv -e .development.env --  node dist/helpers/seeds/seed.js",
        "start:dev": "cross-env NODE_ENV=development dotenv -e .development.env -- concurrently \"tsc -w\" \"nest start --watch\"",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start:test": "nest start -b swc --type-check --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "nest start",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json --watchAll --detectOpenHandles --maxWorkers=6",
        "sentry:sourcemaps": "sentry-cli sourcemaps inject --org david-b8ed80b4b --project p2p_bot ./ && sentry-cli sourcemaps upload --org david-b8ed80b4b --project p2p_bot ./"
